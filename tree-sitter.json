{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ylang",
      "camelcase": "ylang",
      "title": "ylang syntax highlighter",
      "scope": "source.ylang",
      "file-types": [
        "y"
      ],
      "injection-regex": "^ylang$",
      "class-name": "TreeSitterYlang"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for ylang that adds syntax highlighting",
    "authors": [
      {
        "name": "kievzenit",
        "email": "kir.backender@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/kievzenit/tree-sitter-ylang"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": true
  }
}